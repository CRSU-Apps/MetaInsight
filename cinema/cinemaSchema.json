{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "project": {
      "type": "object",
      "properties": {
        "CM": {
          "type": "object",
          "properties": {
            "contributionMatrices": {
              "type": "array",
              "minItems": 1,
              "maxItems": 1,
              "items": {
                "type": "object",
                "properties": {
                  "hatmatrix": {
                    "type": "object",
                    "properties": {
                      "colNames": {
                        "type": "array",
                        "minItems": 1,
                        "uniqueItems": true,
                        "items": {
                          "type": "string",
                          "pattern": "[0-9]+:[0-9]+"
                        }
                      },
                      "colNamesdbt": {
                        "type": "array",
                        "minItems": 3,
                        "maxItems": 3,
                        "uniqueItems": true,
                        "items": {
                          "type": "string",
                          "enum": [
                            "Q_dbt",
                            "df",
                            "pv_dbt"
                          ]
                        }
                      },
                      "colNamesNMAresults": {
                        "type": "array",
                        "minItems": 19,
                        "maxItems": 19,
                        "uniqueItems": true,
                        "items": {
                          "type": "string",
                          "enum": [
                            "Direct",
                            "DirectL",
                            "DirectU",
                            "Indirect",
                            "IndirectL",
                            "IndirectU",
                            "SideIF",
                            "SideIFlower",
                            "SideIFupper",
                            "SideZ",
                            "SidePvalue",
                            "PropDir",
                            "NMA treatment effect",
                            "se treat effect",
                            "lower CI",
                            "upper CI",
                            "lower PrI",
                            "upper PrI",
                            "PropDirNetmeta"
                          ]
                        }
                      },
                      "colNamesPairwise": {
                        "type": "array",
                        "minItems": 4,
                        "uniqueItems": true,
                        "items": {
                          "type": "string",
                          "enum": [
                            "tau2",
                            "I2",
                            "I2 lower",
                            "I2 upper"
                          ]
                        }
                      },
                      "dbt": {
                        "type": "array",
                        "minItems": 1,
                        "items": {
                          "type": "object",
                          "properties": {
                            "df": {
                              "type": "number"
                            },
                            "pv_dbt": {
                              "type": "number"
                            },
                            "Q_dbt": {
                              "type": "number"
                            }
                          }
                        }
                      },
                      "forleaguetable": {
                        "type": "object",
                        "properties": {
                          "lower.fixed": {
                            "type": "array",
                            "items": {
                              "type": "array",
                              "items": {
                                "type": "number"
                              }
                            }
                          },
                          "lower.random": {
                            "type": "array",
                            "items": {
                              "type": "array",
                              "items": {
                                "type": "number"
                              }
                            }
                          },
                          "TE.fixed": {
                            "type": "array",
                            "items": {
                              "type": "array",
                              "items": {
                                "type": "number"
                              }
                            }
                          },
                          "TE.random": {
                            "type": "array",
                            "items": {
                              "type": "array",
                              "items": {
                                "type": "number"
                              }
                            }
                          },
                          "treatnames": {
                            "type": "array",
                            "minItems": 2,
                            "items": {
                              "type": "string",
                              "pattern": "[0-9]+"
                            }
                          },
                          "upper.fixed": {
                            "type": "array",
                            "items": {
                              "type": "array",
                              "items": {
                                "type": "number"
                              }
                            }
                          },
                          "upper.random": {
                            "type": "array",
                            "items": {
                              "type": "array",
                              "items": {
                                "type": "number"
                              }
                            }
                          }
                        },
                        "required": [
                          "lower.fixed",
                          "lower.random",
                          "TE.fixed",
                          "TE.random",
                          "treatnames",
                          "upper.fixed",
                          "upper.random"
                        ]
                      },
                      "forstudycontribution": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "seTE": {
                              "type": "number"
                            },
                            "seTE.adj": {
                              "type": "number"
                            },
                            "studlab": {
                              "type": "string",
                              "pattern": "[0-9]+"
                            },
                            "treat1": {
                              "type": "string",
                              "pattern": "[0-9]+"
                            },
                            "treat2": {
                              "type": "string",
                              "pattern": "[0-9]+"
                            }
                          },
                          "required": [
                            "seTE",
                            "seTE.adj",
                            "studlab",
                            "treat1",
                            "treat2"
                          ]
                        }
                      },
                      "H": {
                        "type": "array",
                        "minItems": 1,
                        "items": {
                          "type": "array",
                          "minItems": 1,
                          "items": {
                            "type": "number"
                          }
                        }
                      },
                      "model": {
                        "type": "array",
                        "minItems": 1,
                        "maxItems": 1,
                        "items": {
                          "type": "string",
                          "enum": [
                            "random",
                            "fixed"
                          ]
                        }
                      },
                      "NMAheterResults": {
                        "type": "array",
                        "minItems": 1,
                        "items": {
                          "type": "object",
                          "properties": {
                            "heterVarNtw": {
                              "type": "number"
                            },
                            "Q heterogeneity": {
                              "type": "number"
                            },
                            "Q inconsistency": {
                              "type": "number"
                            },
                            "Q overall": {
                              "type": "number"
                            }
                          },
                          "required": [
                            "heterVarNtw",
                            "Q heterogeneity",
                            "Q inconsistency",
                            "Q overall"
                          ]
                        }
                      },
                      "NMAresults": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "_row": {
                              "type": "string",
                              "pattern": "[0-9]+:[0-9]+"
                            },
                            "Direct": {
                              "type": "number"
                            },
                            "DirectL": {
                              "type": "number"
                            },
                            "DirectU": {
                              "type": "number"
                            },
                            "Indirect": {
                              "type": "number"
                            },
                            "IndirectL": {
                              "type": "number"
                            },
                            "IndirectU": {
                              "type": "number"
                            },
                            "lower CI": {
                              "type": "number"
                            },
                            "lower PrI": {
                              "type": "number"
                            },
                            "NMA treatment effect": {
                              "type": "number"
                            },
                            "PropDir": {
                              "type": "number"
                            },
                            "PropDirNetmeta": {
                              "type": "number"
                            },
                            "se treat effect": {
                              "type": "number"
                            },
                            "SideIF": {
                              "type": "number"
                            },
                            "SideIFlower": {
                              "type": "number"
                            },
                            "SideIFupper": {
                              "type": "number"
                            },
                            "SidePvalue": {
                              "type": "number"
                            },
                            "SideZ": {
                              "type": "number"
                            },
                            "upper CI": {
                              "type": "number"
                            },
                            "upper PrI": {
                              "type": "number"
                            }
                          },
                          "required": [
                            "_row",
                            "lower CI",
                            "lower PrI",
                            "NMA treatment effect",
                            "PropDir",
                            "PropDirNetmeta",
                            "se treat effect",
                            "upper CI",
                            "upper PrI"
                          ],
                          "dependentRequired": {
                            "Direct": [
                              "DirectL",
                              "DirectU"
                            ],
                            "DirectL": [
                              "Direct",
                              "DirectU"
                            ],
                            "DirectU": [
                              "Direct",
                              "DirectL"
                            ],
                            "Indirect": [
                              "IndirectL",
                              "IndirectU"
                            ],
                            "IndirectL": [
                              "Indirect",
                              "IndirectU"
                            ],
                            "IndirectU": [
                              "Indirect",
                              "IndirectL"
                            ],
                            "SideIF": [
                              "SideIFlower",
                              "SideIFupper",
                              "SidePvalue",
                              "SideZ"
                            ],
                            "SideIFlower": [
                              "SideIF",
                              "SideIFupper",
                              "SidePvalue",
                              "SideZ"
                            ],
                            "SideIFupper": [
                              "SideIF",
                              "SideIFlower",
                              "SidePvalue",
                              "SideZ"
                            ],
                            "SidePvalue": [
                              "SideIF",
                              "SideIFlower",
                              "SideIFupper",
                              "SideZ"
                            ],
                            "SideZ": [
                              "SideIF",
                              "SideIFlower",
                              "SideIFupper",
                              "SidePvalue"
                            ]
                          }
                        }
                      },
                      "Pairwise": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "_row": {
                              "type": "string",
                              "pattern": "[0-9]+ : [0-9]+"
                            },
                            "I2": {
                              "type": "number"
                            },
                            "I2 lower": {
                              "type": "number"
                            },
                            "I2 upper": {
                              "type": "number"
                            },
                            "tau2": {
                              "type": "number"
                            }
                          },
                          "required": [
                            "_row"
                          ]
                        }
                      },
                      "rowNames": {
                        "type": "array",
                        "minItems": 1,
                        "uniqueItems": true,
                        "items": {
                          "type": "string",
                          "pattern": "[0-9]+:[0-9]+"
                        }
                      },
                      "rowNamesNMAresults": {
                        "type": "array",
                        "minItems": 1,
                        "uniqueItems": true,
                        "items": {
                          "type": "string",
                          "pattern": "[0-9]+:[0-9]+"
                        }
                      },
                      "rowNamesPairwise": {
                        "type": "array",
                        "minItems": 1,
                        "uniqueItems": true,
                        "items": {
                          "type": "string",
                          "pattern": "[0-9]+ : [0-9]+"
                        }
                      },
                      "sm": {
                        "type": "array",
                        "minItems": 1,
                        "maxItems": 1,
                        "items": {
                          "type": "string",
                          "enum": [
                            "OR",
                            "RR",
                            "RD",
                            "MD",
                            "SMD"
                          ]
                        }
                      },
                      "tau": {
                        "type": "array",
                        "minItems": 1,
                        "maxItems": 1,
                        "items": {
                          "type": "number"
                        }
                      }
                    },
                    "required": [
                      "colNames",
                      "colNamesdbt",
                      "colNamesNMAresults",
                      "colNamesPairwise",
                      "dbt",
                      "forleaguetable",
                      "forstudycontribution",
                      "H",
                      "model",
                      "NMAheterResults",
                      "NMAresults",
                      "Pairwise",
                      "rowNames",
                      "rowNamesNMAresults",
                      "rowNamesPairwise",
                      "sm",
                      "tau"
                    ]
                  }
                },
                "required": [
                  "hatmatrix"
                ]
              }
            }
          }
        }
      },
      "required": [
        "CM"
      ]
    }
  },
  "required": [
    "project"
  ]
}